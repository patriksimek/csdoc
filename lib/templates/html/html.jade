!!! 5

html
	head
		meta(charset='utf-8')
		title #{title ? title : 'CSDoc'}
		link(rel='stylesheet', href='https://netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css')
		link(rel='stylesheet', href='https://netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap-theme.min.css')
		link(rel='stylesheet', href=level +'csdoc.css')
	
	body
	
		div#wrap
			div.navbar.navbar-default.navbar-fixed-top(role="navigation")
				div.container
					div.navbar-header
						button(type="button", class="navbar-toggle", data-toggle="collapse", data-target=".navbar-collapse")
							span.sr-only Toggle navigation
							span.icon-bar
							span.icon-bar
							span.icon-bar
						
						a.navbar-brand(href=level +'index.html') #{project}
					
					div.collapse.navbar-collapse
						ul.nav.navbar-nav
							li
								a(href=level +'index.html') Classes
							li
								a(href=level +'methods.html') Methods
									
						form.navbar-form.navbar-right
							div(class="form-group")
								label(class="checkbox-inline")
									input(type='checkbox', id='toggle_builtin')
									| Show built-in
			
			div.container
				div.row
					div.col-xs-12.col-sm-4.col-md-3.cs-nav
						block navigation
						
					div.col-xs-12.col-sm-8.col-md-9.cs-main
						block content

		div#footer
			div.container
				p.text-muted.credit Created with 
					a(href='https://github.com/patriksimek/csdoc') CSDoc
			
		!= badge
			
		script(src='https://code.jquery.com/jquery-1.10.2.min.js')
		script(src='https://netdna.bootstrapcdn.com/bootstrap/3.0.2/js/bootstrap.min.js')
		script.
			$('#toggle_builtin').change(function() {
				var enable = $(this).prop('checked');
				
				if (enable) {
					$('.cs-builtin').show();
				} else {
					$('.cs-builtin').hide();
				}
			});