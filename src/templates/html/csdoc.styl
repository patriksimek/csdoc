html,
body
	height: 100%
	background-color #eee

#wrap
	min-height: 100%
	height: auto
	margin: 0 auto -60px
	padding: 0 0 60px

#footer
	height: 60px
	background-color: #f5f5f5

#wrap > .container
	padding: 50px 15px 0

.container .credit
	margin: 20px 0

#footer > .container
	padding-left: 15px
	padding-right: 15px

code
	font-size: 80%

.well
	background transparent
	border none
	border-radius 0
	border-top 1px solid #eee
	box-shadow none
	padding 9px
	margin 0

dl.arguments 
	margin-top 0

	& dt
		text-align left

.arguments-title
	font-weight bold
	margin 15px 0 0 0
	color #ccc

dd > p
	margin 0

.cs-main
	background-color white
	padding 0 30px
	
	& h3
		background-color #eee
		margin 10px -30px 30px -30px
		padding 30px 0 10px 
	
	& h4
		background-color #f6f6f6
		margin 0
		padding 10px 9px
	
	& .cs-defined
		margin 10px 9px

.cs-nav
	padding-top 15px
	
	& h5	
		padding 15px 0 5px 10px
		margin 0
		
		& > a
			color black
			text-transform uppercase
			color #aaa
			font-size 12px
	
	& ul > li > a
		padding 5px 10px

@media (max-width:767px)
	.cs-main
		& h3
			padding-left 40px